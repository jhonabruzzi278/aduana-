<section class="my-6 bg-white p-4 rounded shadow">
  <h3 class="text-lg font-semibold mb-4">Registrar Ingreso de Vehículo</h3>
  <form id="form-vehiculo" class="space-y-4">
    <div>
      <label for="placa" class="block font-medium">Placa Patente</label>
      <input id="placa" type="text" class="w-full p-2 border border-gray-300 rounded" />
    </div>
    <div>
      <label for="marca" class="block font-medium">Marca</label>
      <input id="marca" type="text" class="w-full p-2 border border-gray-300 rounded" />
    </div>
    <div>
      <label for="modelo" class="block font-medium">Modelo</label>
      <input id="modelo" type="text" class="w-full p-2 border border-gray-300 rounded" />
    </div>
    <button type="submit" class="bg-yellow-600 text-white px-4 py-2 rounded hover:bg-yellow-500">Registrar Vehículo</button>
  </form>
  <p id="resultado-vehiculo" class="mt-4 font-semibold"></p>
</section>

<script type="module">
  import { setupFormularioVehiculo } from "/src/scripts/formularioVehiculo.ts";
  setupFormularioVehiculo();
</script>

